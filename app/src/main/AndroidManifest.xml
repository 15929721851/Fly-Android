<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fly.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name="com.fly.example.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.fly.example.ui.livebustest.LiveBusFirstActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:taskAffinity="" />

        <activity
            android:name="com.fly.example.ui.livebustest.LiveBusSecondActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:taskAffinity="" />


        <activity
            android:name="com.fly.example.ui.CustomWidgetTestActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:taskAffinity="" />

        <activity
            android:name="com.fly.example.ui.CoroutineTestActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:taskAffinity="" />


        <activity
            android:name="com.fly.example.ui.ItemDecorationTestActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:taskAffinity="" />

        <activity
            android:name="com.fly.example.ui.ChartActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:taskAffinity="" />

    </application>
</manifest>